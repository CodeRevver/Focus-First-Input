{
  "name": "Focus on first input (keyboard shortcut)",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "Adds a shortcut that focuses cursor in first text input.",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "inj.js"
      ],      
      "css" : ["inj.css"]
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'"
}